    "test:e2e-watch": "jest -c e2e/jest.config.js --watch",
    "test:debug": "react-scripts --inspect-brk test --runInBand --no-cache",
    "build:serve": "pnpm build && serve -s build",
    "build:profile": "pnpm build --profile && serve -s build",
    "analyze": "pnpm build && source-map-explorer 'build/static/js/*.js'",
    "analyzer": "pnpm build && npx cra-bundle-analyzer"
  },
