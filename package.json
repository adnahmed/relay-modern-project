    "fix": "eslint --ext .js,.jsx,.ts,.tsx  --fix ./",
    "format": "prettier --write 'src/**/*.{ts,tsx,scss,css,json}'",
    "isready": "pnpm format && pnpm lint && pnpm build",
    "coverage": "pnpm test -- --coverage",
    "test:clean": "rimraf ./coverage",
